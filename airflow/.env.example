# Airflow environment variables
# Copy this file to .env and fill in your credentials.

# Snowflake connection (used by SnowflakeOperator)
# Format: snowflake://<user>:<password>@<account>/<database>/<schema>?warehouse=<wh>&role=<role>
AIRFLOW_CONN_SNOWFLAKE_DEFAULT=snowflake://USER:PASSWORD@ACCOUNT/NIMBUSBILL/PUBLIC?warehouse=COMPUTE_WH&role=SYSADMIN&account=ACCOUNT

# AWS connection (for S3 staging, if applicable)
# Format: aws://<access_key_id>:<secret_access_key>@?region_name=<region>
AIRFLOW_CONN_AWS_DEFAULT=aws://ACCESS_KEY_ID:SECRET_ACCESS_KEY@?region_name=us-east-1
